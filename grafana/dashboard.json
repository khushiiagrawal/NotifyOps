      },
      {
        "id": 6,
        "title": "Slack Message Delivery",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(slack_messages_sent_total[5m])",
            "legendFormat": "{{channel}} - {{message_type}} - {{status}}"
